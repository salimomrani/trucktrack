# =============================================================================
# Environment Variables Template
# =============================================================================
# Copy this file to .env.local for local development
# In production, set these as environment variables
# =============================================================================

# SonarQube Configuration
SONAR_TOKEN=your-sonarqube-token-here
SONAR_HOST_URL=http://localhost:9001

# Database
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=trucktrack
# DB_USER=trucktrack
# DB_PASSWORD=your-password-here

# Mailjet Configuration (Feature 016 - Email Notifications)
MAILJET_API_KEY=your-mailjet-api-key
MAILJET_API_SECRET=your-mailjet-api-secret
MAILJET_FROM_EMAIL=notifications@yourdomain.com
MAILJET_FROM_NAME=TruckTrack

# Firebase Configuration (Feature 016 - Push Notifications)
FIREBASE_ENABLED=false
FIREBASE_CREDENTIALS_PATH=/path/to/firebase-credentials.json
